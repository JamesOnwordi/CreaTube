    </head>



    <%if(armory.displayName =="Melee"){%>
      <div class="container text-center">
        <div class="row">
          <div class="col">
            <div class="display">
            </div>
          </div>
          <div class="col">
            <div class="display">
              <div>
                <div><h1><%= armory.displayName %></h1>
            </div>
            <div ><img src="<%=armory.displayIcon%>"></div>
            <div>
              <form action="<%=armory.uuid%>" method="post">
                <button>Add to favorite</button>
              </form>
            </div>
              </div>
              
            </div>
            </div>
            
          <div class="col">
            <div class="display">
              
              
             </div>
          </div>
        </div>
      </div>
      <% }else{ %> 

    <div class="container text-center">
      <div class="row">
        <div class="col">
          <div class="display">
            <h5><%=armory.shopData.category%></h5>
          </div>
        </div>
        <div class="col">
          <div class="display">
            <div>
              <div><h1><%= armory.displayName %></h1>
          </div>
          <div ><img src="<%=armory.displayIcon%>"></div>
          <div>
            <form action="<%=armory.uuid%>" method="post">
              <button>Add to favorite</button>
            </form>
          </div>
            </div>
            
          </div>
          </div>
          
        <div class="col">
          <div class="display">
            <div>
            <h6>Head Damage: <%=armory.weaponStats.damageRanges[0].headDamage%></h6>
            <h6>Body Damage: <%=armory.weaponStats.damageRanges[0].bodyDamage%></h6>
            <h6>Leg Damage: <%=armory.weaponStats.damageRanges[0].legDamage%></h5>
            </div>
            
           </div>
        </div>
      </div>
    </div>




    
    
  </div></div>

  <div class="table-responsive">
    <table class="table">
      <thead>
        <tr>
          <%if(armory.weaponStats.fireRate){%>
          <th scope="col">fireRate</th>
          <% } %> 
          <%if(armory.weaponStats.magazineSize){%>
          <th scope="col">magazineSize</th>
          <% } %> 
          <%if(armory.weaponStats.runSpeedMultiplier){%>
          <th scope="col">runSpeedMultiplier</th>
          <% } %> 
          <%if(armory.weaponStats.equipTimeSeconds){%>
          <th scope="col">equipTimeSeconds</th>
          <% } %> 
          <%if(armory.weaponStats.firstBulletAccuracy){%>
          <th scope="col">firstBulletAccuracy</th>
          <% } %> 
          <%if(armory.weaponStats.shotgunPelletCount){%>
          <th scope="col">shotgunPelletCount</th>
          <% } %> 
          <%if(armory.weaponStats.wallPenetration){%>
          <th scope="col">wallPenetration</th>
          <% } %> 
          <%if(armory.weaponStats.fireMode){%>
          <th scope="col">fireMode</th>
          <% } %> 
        </tr>
      </thead>
      <tbody>
        <tr>
          <%if(armory.weaponStats.fireRate){%>
          <td><%=armory.weaponStats.fireRate%> rounds/sec</td>
          <% } %> 
          <%if(armory.weaponStats.magazineSize){%>
          <td><%=armory.weaponStats.magazineSize%></td>
          <% } %> 
          <%if(armory.weaponStats.runSpeedMultiplier){%>
          <td>x<%=armory.weaponStats.runSpeedMultiplier%></td>
          <% } %> 
          <%if(armory.weaponStats.equipTimeSeconds){%>
          <td><%=armory.weaponStats.equipTimeSeconds%> sec</td>
          <% } %> 
          <%if(armory.weaponStats.firstBulletAccuracy){%>
          <td><%=armory.weaponStats.firstBulletAccuracy%></td>
          <% } %> 
          <%if(armory.weaponStats.shotgunPelletCount){%>
          <td><%=armory.weaponStats.shotgunPelletCount%></td>
          <% } %> 
          <%if(armory.weaponStats.wallPenetration){%>
          <td><%=armory.weaponStats.wallPenetration%></td>
          <% } %> 
          <%if(armory.weaponStats.fireMode){%>
            <td><%=armory.weaponStats.fireMode%></td>
          <% } %> 
        </tr>
      </tbody>
    </table>
  </div>
  <% } %> 
  
