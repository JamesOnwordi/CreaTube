
<h2>Favorites</h2>

<% favorites.forEach(favorite =>{%> 
    <div>
        <ul>
               <li>
                <p> <%= favorite.name %></p>
                <img src="<%= favorite.image %>" alt=""></p>
                <a href="favorite/<%=favorite.uuid%>"><button>Get Details</button></a>
                <a href="favorite/skin/<%=favorite.uuid%>"><button> Change Skin</button></a>  
                <a href="/users"><button>Add To Favorite</button></a>
                <form action="favorite/<%= favorite.id %>/?_method=DELETE" method="post">
                        <p></p>
                        <button>Remove Favorite</button>
                </form>
                <a href="favorite/skin/video<%=favorite.uuid%>"><button>Watch Video</button></a>
                    
               </li>
        </ul>
    </div>
    <% }) %> 

    

   
    